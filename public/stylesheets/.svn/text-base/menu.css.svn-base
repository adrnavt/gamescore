.menu_admin{
	border:none;
	border:0px;
	margin:0px;
	padding:0px;
	font: 67.5% 'TitilliumText22LMedium', Arial , sans-serif;
	font-size:14px;
	font-weight:bold;
	}
.menu_admin_admin ul{
	height:35px;
	list-style:none;
	margin:0;
	padding:0;
	}
	.menu_admin li{
		float:left;
		padding:0px;
		}
	.menu_admin li a{
		color:#c00;
		display:block;
		font-weight:normal;
		line-height:35px;
		margin:0px;
		padding:0px 20px;
		text-align:center;
		text-decoration:none;
		}
		.menu_admin li a:hover, .menu_admin ul li:hover a{
			background: #c00;
			color:#FFFFFF;
			text-decoration:none;
			-webkit-border-top-right-radius: 3px;
			-moz-border-top-right-radius: 3px;
			border-top-right-radius: 3px;
			-webkit-border-top-left-radius: 3px;
			-moz-border-top-left-radius: 3px;
			border-top-left-radius: 3px;
			}
	.menu_admin li ul{
		background:#c00;
		display:none;
		height:auto;
		padding:0px;
		margin:0px;
		border:0px;
		position:absolute;
		width:225px;
		z-index:200;
		/*top:1em;
		/*left:0;*/
		-webkit-border-top-right-radius: 3px;
		-moz-border-top-right-radius: 3px;
		border-top-right-radius: 3px;
		-webkit-border-bottom-right-radius: 3px;
		-moz-border-bottom-right-radius: 3px;
		border-bottom-right-radius: 3px;
		-webkit-border-bottom-left-radius: 3px;
		-moz-border-bottom-left-radius: 3px;
		border-bottom-left-radius: 3px;
		}
	.menu_admin li:hover ul{
		display:block;
		
		}
	.menu_admin li li {
		display:block;
		float:none;
		margin:0px;
		padding:0px;
		width:225px;
		}
	.menu_admin li:hover li a{
		background:none;
		
		}
	.menu_admin li ul a{
		display:block;
		height:35px;
		font-size:12px;
		font-style:normal;
		margin:0px;
		padding:0px 20px;
		text-align:left;
		}
		.menu_admin li ul a:hover, .menu_admin li ul li:hover a{
			background:#900;
			border:0px;
			color:#ffffff;
			text-decoration:none;
			}
	.menu_admin p{
		clear:left;
		}	

